XTerm.termName:          xterm-256color
UXTerm.termName:         xterm-256color
*.vt100.background:      Black
*.vt100.foreground:      White
*.vt100.faceName:        DejaVu Sans Mono:size=10
*.vt100.saveLines:       8192
*.vt100.cursorBlink:     false
*.vt100.metaSendsEscape: true

! black
*.vt100.color0  : #2E3436
*.vt100.color8  : #555753
! red
*.vt100.color1  : #CC0000
*.vt100.color9  : #EF2929
! green
*.vt100.color2  : #4E9A06
*.vt100.color10 : #8AE234
! yellow
*.vt100.color3  : #C4A000
*.vt100.color11 : #FCE94F
! blue
*.vt100.color4  : #3465A4
*.vt100.color12 : #729FCF
! magenta
*.vt100.color5  : #75507B
*.vt100.color13 : #AD7FA8
! cyan
*.vt100.color6  : #06989A
*.vt100.color14 : #34E2E2
! white
*.vt100.color7  : #D3D7CF
*.vt100.color15 : #EEEEEC

